<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.backend.videoproject_backend.dto.TbAssociationEntity" table="tb_association" schema="bili">
        <id name="id" column="id"/>
        <property name="associationName" column="association_name"/>
        <property name="establishTime" column="establish_time"/>
        <property name="memberNum" column="member_num"/>
        <property name="associationDesc" column="association_desc"/>
    </class>
</hibernate-mapping>